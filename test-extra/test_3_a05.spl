struct Student
{
    int ID;
    int score;
};

int test(struct Student* ptr) {
    write(ptr->ID);
    write(ptr->score);
    ptr->ID++;
    write(ptr->ID);
    return ptr->ID;
}

int main()
{
    struct Student s1, s2;
    s1.ID = 1;
    s1.score = 70;
    s2.ID = 2;
    s2.score = 90;
    write(s2.ID);
    write(s1.score);
    struct Student * ptr;
    ptr = &s2;
    test(ptr);
    write(ptr->score);
    write(ptr->ID);

    struct Student stus[10];
    for(int i=0;i<10;i++) {
        stus[i].ID = i + 101;
        stus[i].score = 123;
        test(&stus[i]);
        test(&stus[i]);
    }
    return 0;
}

