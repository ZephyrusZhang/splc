struct Student
{
    int ID;
    int score;
};

int test(struct Student* ptr) {
    write(ptr->ID);
    write(ptr->score);
    ptr->ID++;
    return ptr->ID;
}

int main()
{
    struct Student s1, s2;
    s1.ID = 1;
    s1.score = 70;
    s2.ID = 2;
    s2.score = 90;
    write(s2.ID);
    write(s1.score);
    struct Student * ptr;
    ptr = &s2;
    write(ptr->score);
    write(ptr->ID);
    return 0;
}

